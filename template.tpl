if (getType(products) === 'string') {
  var temp = products;

  while (temp.indexOf(',}') !== -1) temp = temp.split(',}').join('}');
  while (temp.indexOf(',]') !== -1) temp = temp.split(',]').join(']');

  // Optional check for control characters
  var safe = true;
  for (var i = 0; i < temp.length; i++) {
    var c = temp.charCodeAt(i);
    if (c < 32 && c !== 9 && c !== 10 && c !== 13) {
