    adjustedRevenue = revenue - shipping - tax;
    adjustedTax = tax;
    debugLog('adjustedRevenue (End 8): ' + adjustedRevenue);
    debugLog('adjustedTax (End 8): ' + adjustedTax);
    return calculateTaxRate(adjustedTax, adjustedRevenue);
