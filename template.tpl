  if (doesRevenueInclTax) {
    adjustedRevenue = revenue - tax;
    debugLog('adjustedRevenue (End2): ' + adjustedRevenue);
    debugLog('adjustedTax (End 2): ' + adjustedTax);
    return calculateTaxRate(adjustedTax, adjustedRevenue);
  }

